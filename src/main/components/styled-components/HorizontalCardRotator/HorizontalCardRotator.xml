<?xml version="1.0" encoding="utf-8" ?>
<component name="HorizontalCardRotator" extends="Panel" >
    <script type="text/brightscript" uri="HorizontalCardRotator.brs" />
    <interface>
        <field
            id="content"
            type="node"
            onChange="updateContents"
            value = "[]"
        />
        <field
            id="targetSet"
            type="node"
            onChange="updateTargetSet"
            value = "[]"
        />
    </interface>
    <children>
        <TargetList id="HorizontalCardRotatorList" defaultTargetSetFocusIndex="1" advanceKey="right" reverseKey="left" />
    </children>
</component>